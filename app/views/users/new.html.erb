<h1>new.html.erb</h1>

<%= form_for @user do |user_form| %>
  <%= user_form.text_field :username, :placeholder => "Username" %><br>
  <%= user_form.text_field :email, :placeholder => "Email" %><br>
  <%= user_form.password_field :password, :placeholder => "Password" %><br>
  <%= user_form.submit "Create User" %><br>
<% end %>
